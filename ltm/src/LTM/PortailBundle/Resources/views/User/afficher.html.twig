        $(document).ready(function(){

            $(".age").each(function() {
                $(this).html(getAge($(this).html()))
            });

            $(".sincedate").each(function() {
                // $(this).html(getSinceDate($(this).html()))
            });


            function getAge(dateString)
            {
                var today = new Date();
                var birthDate = new Date(dateString);
                var age = today.getFullYear() - birthDate.getFullYear();
                var m = today.getMonth() - birthDate.getMonth();
                if (m < 0 || (m === 0 && today.getDate() < birthDate.getDate()))
                {
                    age--;
                }
                return age;
            }



            function inSecondes (d1, d2) {
                var t2 = d2.getTime();
                var t1 = d1.getTime();

                return parseInt((t2-t1)/(1000));
            }

            function inMinutes (d1, d2) {
                var t2 = d2.getTime();
                var t1 = d1.getTime();

                return parseInt((t2-t1)/(60*1000));
            }
            function inHours (d1, d2) {
                var t2 = d2.getTime();
                var t1 = d1.getTime();

                return parseInt((t2-t1)/(3600*1000));
            } function inDays (d1, d2) {
                var t2 = d2.getTime();
                var t1 = d1.getTime();

                return parseInt((t2-t1)/(24*3600*1000));
            }

            function inWeeks (d1, d2) {
                var t2 = d2.getTime();
                var t1 = d1.getTime();

                return parseInt((t2-t1)/(24*3600*1000*7));
            }

            function inMonths (d1, d2) {
                var d1Y = d1.getFullYear();
                var d2Y = d2.getFullYear();
                var d1M = d1.getMonth();
                var d2M = d2.getMonth();

                return (d2M+12*d2Y)-(d1M+12*d1Y);
            }

            function inYears (d1, d2) {
                return d2.getFullYear()-d1.getFullYear();
            }



            function getSinceDate(dateString)
            {
                var d1 = new Date("{{ "now" | date ('Y-m-d H:i:s') }}");
                var d2 = new Date(dateString);
                secondes = inSecondes(d1, d2);
                minutes =  inMinutes(d1, d2);
                hours =  inHours(d1, d2);
                days =  inDays(d1, d2);
                weeks =  inWeeks(d1, d2);
                months =  inMonths(d1, d2);
                years =  inYears(d1, d2);

                result = "";
                if (years > 0) result += years + ' ans ';
                if (months > 0) result +=  months + ' mois ';
                if (weeks > 0) result +=  weeks + ' semaines ' ;
                if (days > 0) result += days + ' jours ' ;
                if (hours > 0) result +=  hours + ' heures ' ;
                if (minutes > 0) result += minutes + ' minutes ' ;
                return result;
                //return secondes + ' secondes';

            }



        });



