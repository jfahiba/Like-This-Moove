<!-- app/Resources/views/base.html.twig -->
<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html"; charset=utf-8" />
    <title>{% block title %}Like This Moove | Lancement du nouveau site..{% endblock %}</title>
    <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
    {% block stylesheets %}
       <!-- Meta -->
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="description">
    <meta content="" name="author">

    <!-- CSS Global Compulsory-->
    <link href="{{ asset('bundles/portail/plugins/bootstrap/css/bootstrap.min.css')}}" rel="stylesheet">
    <link href="{{ asset('bundles/portail/css/style.css')}}" rel="stylesheet">
    <link href="{{ asset('bundles/portail/plugins/bootstrap/css/bootstrap-responsive.min.css')}}" rel="stylesheet">
    <link href="{{ asset('bundles/portail/css/style_responsive.css')}}" rel="stylesheet">
    <link href="favicon.ico" rel="shortcut icon">        
    <!-- CSS Implementing Plugins -->    
    <link href="{{ asset('bundles/portail/plugins/font-awesome/css/font-awesome.css')}}" rel="stylesheet">
    <link href="{{ asset('bundles/portail/plugins/countdown/css/countdown.css')}}" rel="stylesheet">
    <!-- CSS Theme -->    
    <link id="style_color" href="{{ asset('bundles/portail/css/themes/default.css')}}" rel="stylesheet">
    <link id="style_color-header-1" href="{{ asset('bundles/portail/css/themes/headers/default.css')}}" rel="stylesheet"> 
    {% endblock %}
    {% block favicon %}

    {% endblock %}

</head>
<body class="coming-soon-page">



<div class="coming-soon-border"></div>

<!--=== Content Part ===-->    
<div class="container">
    <!-- Coming Soon Content -->
    <div class="row-fluid">
        <div class="span12 coming-soon">
            <h1>Lancement du nouveau site!</h1>
            <p>Likethismoove All Dances All styles Dance Social Network.</p><br>
            <!--form class="form-search">
                <input type="text" placeholder="Your Email" class="input-large search-query">
                <button class="btn-u" type="submit">Subscribe</button>
            </form-->
			{%  render(controller('PortailBundle:Default:newsletter')) %}
        </div>
    </div>

    <!-- Coming Soon Plugn -->
    <div class="row-fluid">    
        <div class="coming-soon-plugin">
			<div id="defaultCountdown"></div>
        </div>
    </div>

    <!-- Coming Soon Footer -->
    <div class="row-fluid">
        <div class="span6">
            <ul class="social-icons">
                <li><a href="https://www.facebook.com/LIKETHISMOOVE?ref=hl" data-original-title="Facebook" class="social_facebook"></a></li>
				<li><a href="https://twitter.com/Likethismoove" data-original-title="Twitter" class="social_twitter"></a></li>

				<li><a href="http://pinterest.com/likethismoove/boards/" data-original-title="Pinterest" class="social_pintrest"></a></li>
				<li><a href="http://vimeo.com/user4117329" data-original-title="Vimeo" class="social_vimeo"></a></li>
            </ul>            
        </div>
        <div class="span6 coming-soon-copyright">
            <p>2014 &copy; LikeThisMoove - All Dances All styles Dance Social Network</p>
        </div>
    </div>
</div><!--/container-->
<!--=== End Content Part ===-->


<!-- JS Global Compulsory -->           
<script src="{{ asset('bundles/portail/js/jquery-1.8.2.min.js')}}" type="text/javascript"></script>
<script src="{{ asset('bundles/portail/js/modernizr.custom.js')}}" type="text/javascript"></script>        
<script src="{{ asset('bundles/portail/plugins/bootstrap/js/bootstrap.min.js')}}" type="text/javascript"></script> 
<!-- JS Implementing Plugins -->           
<script src="{{ asset('bundles/portail/plugins/countdown/jquery.countdown.js')}}" type="text/javascript"></script>
<script src="{{ asset('bundles/portail/plugins/backstretch/jquery.backstretch.min.js')}}" type="text/javascript"></script>
<script src="{{ asset('bundles/portail/plugins/back-to-top.js')}}" type="text/javascript"></script>
<!-- JS Page Level -->           
<script src="{{ asset('bundles/portail/js/app.js')}}" type="text/javascript"></script>

<script>
    $.backstretch([
      "{{ asset('bundles/portail/img/bg/1.png')}}",
      "{{ asset('bundles/portail/img/bg/3.png')}}",
      "{{ asset('bundles/portail/img/bg/2.png')}}"
      ], {
        fade: 1000,
        duration: 7000
    });
</script>
<div class="backstretch"></div>



<script type="text/javascript">
    jQuery(document).ready(function() {
        App.init();
		
		$(function () {
        var austDay = new Date();
        austDay = new Date(austDay.getFullYear() + 0, 9 - 1, 29, 6);
        $('#defaultCountdown').countdown({until: austDay});
        $('#year').text(nextgame.getFullYear());
    });
    });
</script>


 <div id="topcontrol" style="position: fixed; bottom: 5px; right: 5px; opacity: 0; cursor: pointer;" 
 title="Scroll Back to Top"><img style="width:51px; height:42px" src="{{ asset('bundles/portail/img/up.png')}}"></div>

</body>
</html>




